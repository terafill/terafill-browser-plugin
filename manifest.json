{
  "manifest_version": 3,
  "name": "Terafill",
  "version": "1.0",
  "action": {
    "default_title": "Terafill: Login faster",
    "default_popup": "index.html",
    "default_icon": {
      "16": "Logo-mini-derived.png",
      "48": "Logo-mini-derived.png",
      "128": "Logo-mini-derived.png"
    }
  },
  "web_accessible_resources": [
      {
        "resources": [
          "Logo-mini-derived.png"
        ],
        "matches": ["<all_urls>"],
        "extension_ids": ["*"]
      }
  ],
  "permissions": ["activeTab", "tabs", "storage", "cookies", "http://0.0.0.0:8000", "runtime"],
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": ["<all_urls>"],
      "js": ["cs.js"],
      "css": ["cs.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
    }
    
}
